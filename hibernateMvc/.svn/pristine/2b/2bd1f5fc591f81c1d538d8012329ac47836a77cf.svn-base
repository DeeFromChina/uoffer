<sqls>
	<sql orderBy="id">
		SELECT U.OFFER_USER_NAME, U.OFFER_USER_EMAIL, U.OFFER_JOB_ID, U.OFFER_USER_CITY, U.OFFER_USER_PROVINCE,
			I.OFFER_USER_NOW_YEAR_WAGES, I.OFFER_USER_EDUCATION, I.OFFER_USER_ADDRESS, I.OFFER_USER_WORK_PLACE,
			I.OFFER_USER_PLAN_YEAR_WAGES, I.OFFER_USER_SEX, I.OFFER_USER_DESCRIPTION, I.OFFER_ADVISER_REMARK
		FROM offer_collect C
		LEFT JOIN offer_user U ON C.FIRM_ID = U.FIRM_ID
		LEFT JOIN offer_user_infomation I ON I.OFFER_USER_ID = U.ID
		{#param}
		<param value="param"/>
	</sql>
</sqls>