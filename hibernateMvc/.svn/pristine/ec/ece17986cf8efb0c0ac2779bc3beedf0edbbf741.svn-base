<script language="javascript">
	function upload() {
		$.ajax({
			url : "register/firm.do", // 自行按需配置好完整的url，包括ip和端口号  
			type : "POST",
			datatype : "text",
			data : JSON.stringify($("#form1").serializeObject()),
			contentType : "application/json",
		});
	}
	//头部
