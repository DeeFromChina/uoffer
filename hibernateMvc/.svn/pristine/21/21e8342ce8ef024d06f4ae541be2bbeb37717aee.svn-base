package com.offer.model;

import javax.persistence.Column;
import javax.persistence.Entity;
import javax.persistence.GeneratedValue;
import javax.persistence.Id;
import javax.persistence.Table;

@Entity
@Table(name = "offer_user_education")
public class OfferUserEducation {

   @Id
   @GeneratedValue
   @Column(name = "id")
   private Integer id;

   @Column(name = "school_name")
   private String  schoolName;

   @Column(name = "star_time")
   private String  starTime;

   @Column(name = "end_time")
   private String  endTime;

   @Column(name = "user_major")
   private String  userMajor;

   @Column(name = "offer_user_id")
   private String  offerUserId;

   @Column(name = "offer_user_xueli")
   private String  offerUserXueli;

   @Column(name = "to_now")
   private String  toNow;

   public Integer getId() {
      return id;
   }

   public void setId(Integer id) {
      this.id = id;
   }

   public String getSchoolName() {
      return schoolName;
   }

   public void setSchoolName(String schoolName) {
      this.schoolName = schoolName;
   }

   public String getStarTime() {
      return starTime;
   }

   public void setStarTime(String starTime) {
      this.starTime = starTime;
   }

   public String getEndTime() {
      return endTime;
   }

   public void setEndTime(String endTime) {
      this.endTime = endTime;
   }

   public String getUserMajor() {
      return userMajor;
   }

   public void setUserMajor(String userMajor) {
      this.userMajor = userMajor;
   }

   public String getOfferUserId() {
      return offerUserId;
   }

   public void setOfferUserId(String offerUserId) {
      this.offerUserId = offerUserId;
   }

   public String getOfferUserXueli() {
      return offerUserXueli;
   }

   public void setOfferUserXueli(String offerUserXueli) {
      this.offerUserXueli = offerUserXueli;
   }

   public String getToNow() {
      return toNow;
   }

   public void setToNow(String toNow) {
      this.toNow = toNow;
   }
}
